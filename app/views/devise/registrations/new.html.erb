<div class="card">
<header class="card-header">
  <h4 class="card-title text-center">Sign up</h4>
</header>
<article class="card-body">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>
  <div class="form-row">
    <div class="col form-group">
      <%= f.label :fullname, style:"font-size:15px;" %>  
      <%= f.text_field :fullname, class:"form-control", style:"font-size:15px;" %>
    </div> <!-- form-group end.// -->
    <div class="col form-group">
      <%= f.label :email, style:"font-size:15px;" %>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", style:"font-size:15px;", class:"form-control" %>
        <small class="form-text text-muted", style="font-size:15px;">We'll never share your email with anyone else.</small>
    </div> <!-- form-group end.// -->
  </div> <!-- form-row end.// -->
  <div class="form-group">
      <%= f.label :phonenumber, style:"font-size:15px;" %>  
      <%= f.text_field :phonenumber, class:"form-control", style:"font-size:15px;" %>
  </div> <!-- form-group end.// -->
  <div class="form-group">
    <%= f.label :gender, style:"font-size:15px;" %>
    <%= f.radio_button :gender, 'Male' %> <strong style="font-size: 15px;">Male</strong> <%= f.radio_button :gender, 'Female' %> <strong style="font-size: 15px;">Female</strong>
  </div> <!-- form-group end.// -->
  <%# <script>
    var stateObject = {
      "Rwanda": { 
      "Kigali City": ["Kicukiro", "Nyarugenge", "Gasabo"],
      "Eastern Province": ["Nyagatare", "Rwamagana", "Ngoma", "Kayonza", "kirehe", "Gatsibo", "Bugesera"],
      "Western Province": ["Ngororero", "Karongi", "Rusizi", "Rubavu", "Rutsiro", "Nyabihu", "Nyamasheke"],
      "Southern Province Province": ["Gisagara", "Huye", "Kamonyi", "Nyanza", "Muhanga", "Ruhango", "Nyaruguru", "Nyamagabe"],
      "Northern Province": ["Rulindo", "Gakenke", "Musanze", "Gicumbi", "Burera"],
      },
      "Tanzania": {
      "": ["", ""],
      "": ["", ""]
      },
      "Uganda": {
      "": ["", ""],
      "": ["", ""]
      },
      "Burundi": {
      "": ["", ""],
      "": ["", ""]
      },
      "Kenya": {
      "": ["", ""],
      "": ["", ""]
      },
      }
      window.onload = function () {
      var countrySel = document.getElementById("countrySel"),
      stateSel = document.getElementById("stateSel"),
      districtSel = document.getElementById("districtSel");
      for (var country in stateObject) {
      countrySel.options[countrySel.options.length] = new Option(country, country);
      }
      countrySel.onchange = function () {
      stateSel.length = 1; // remove all options bar first
      districtSel.length = 1; // remove all options bar first
      if (this.selectedIndex < 1) return; // done
      for (var state in stateObject[this.value]) {
      stateSel.options[stateSel.options.length] = new Option(state, state);
      }
      }
      countrySel.onchange(); // reset in case page is reloaded
      stateSel.onchange = function () {
      districtSel.length = 1; // remove all options bar first
      if (this.selectedIndex < 1) return; // done
      var district = stateObject[countrySel.value][this.value];
      for (var i = 0; i < district.length; i++) {
      districtSel.options[districtSel.options.length] = new Option(district[i], district[i]);
      }
      }
      }
  </script> %>
  <div class="form-group">
    <%= f.label :country, style:"font-size:15px;" %>  
    <%= f.text_field :country, class:"form-control", style:"font-size:15px;" %>
  </div>
  <div class="form-group">
    <%= f.label :province,style:"font-size:15px;" %>
    <%= f.text_field :province, class:"form-control", style:"font-size:15px;" %>
  </div>
  <div class="form-group">
    <%= f.label :district, style:"font-size:15px;" %>
    <%= f.text_field :district, class:"form-control", style:"font-size:15px;" %>
  </div>
  <div class="form-row">
    <div class="col form-group"> 
      <%= f.label :password, style:"font-size:15px;" %>
      <% if @minimum_password_length %>
      <em style="font-size: 15px;">(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password", style:"font-size:15px;", id:"psw", class:"form-control" %>
    </div> <!-- form-group end.// -->
    <div class="col form-group">
     <%= f.label :password_confirmation, style:"font-size:15px;" %><br />
     <%= f.password_field :password_confirmation, autocomplete: "new-password", style:"font-size:15px;", id:"psw", class:"form-control" %>
    </div> <!-- form-group end.// -->
  </div> <!-- form-row end.// -->
  <div id="message">
  <h6>Password must contain the following:</h6>
    <p id="letter" class="invalid">A <b>lowercase</b> letter</p>
    <p id="capital" class="invalid">A <b>capital (uppercase)</b> letter</p>
    <p id="number" class="invalid">A <b>number</b></p>
    <p id="length" class="invalid">Minimum <b>8 characters</b></p>
  </div>
  <small class="text-muted" style="font-size: 15px;">By clicking the 'Sign Up' button, you confirm that you accept our <br> Terms of use and Privacy Policy.</small>
  <div class="form-group">
    <%= f.submit "Sign up", class:"btn btn-primary btn-block", style:"font-size:15px;" %>
  </div> <!-- form-group// -->      
<% end %>
</article> <!-- card-body end .// --> 
<div class="border-top card-body text-center" style="font-size: 15px;">Have an account?
  <%= render "devise/shared/links" %>
</div>
</div> <!-- card.// -->
</div> <!-- col.//-->
</div> <!-- row.//-->
</div> 

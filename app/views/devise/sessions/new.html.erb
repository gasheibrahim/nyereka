															<li><%= link_to "Tutorials", tutorials_index_path %></li>
                              <li><%= link_to "Training", training_index_path %></li>
															<li><%= link_to "Project Assistance", project_assistance_index_path %></li>
														</ul>
													</li>
												</ul>
										</div>
									</div>
									
								</nav>
                <!--/ End Main Menu -->
              </div>
              
            </div>
          </div>
          
        </div>
      </div>
    </div>
    <!--/ End Header Inner -->
    <div class="middle-inner" style="background: #dfe7e9;">
      <div class="container">
        <div class="row">
          <div class="col-lg-2 col-md-2 col-12" >
            <!-- Logo -->
            <div class="logo" style="margin-top:-10px;">
              <%= link_to root_url do %>
							  <%= image_tag("PNG.png", alt:"logo") %>
							<% end %>
            </div>
            <!--/ End Logo -->
            <div class="mobile-nav"></div>
          </div>
          
          <div class="col-lg-2 col-md-12 col-12">

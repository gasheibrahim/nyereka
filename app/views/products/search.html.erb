<body class="js">
  <header class="header shop">
    <!-- Topbar -->
    <div class="middle-inner">
      <div class="container">
        <div class="row">
          <div class="col-lg-2 col-md-2 col-12" >
            <!-- Logo -->
            <div class="logo" style="margin-top:-10px;">
              <%= link_to root_url do %>
							  <%= image_tag("PNG.png", alt:"logo") %>
							<% end %>
            </div>
            <!--/ End Logo -->
            <!-- Search Form -->
            <div class="search-top" style="margin-right: 20px;">
              <div class="top-search"><a href="#"><i class="fas fa-search" style="color:#fff;font-size: 30px;margin-top: 10px;margin-left: 80px;"></i></a></div>
              <!-- Search Form -->
              <div class="search-top">
              <%= form_with url: products_path, local: true, method: :get do |form| %>
              <%= form.text_field :q, placeholder: "I Want To Buy..........", style:"height: 50px;width:190px;font-size:15px;" %>
                <% end %>
              </div>
              <!--/ End Search Form -->
            </div>
            <!--/ End Search Form -->
            <div class="mobile-nav"></div>
          </div>
          <div class="col-lg-2 col-md-12 col-12">
            <div class="right-bar">
              <!-- Search Form -->
              <strong class="nyereka tech">Nyereka Tech</strong>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Header Inner -->
    <div class="header-inner" style="height:62px;">
      <div class="container">
        <div class="cat-nav-head">
          <div class="row">
            <div class="col-lg-9 col-12">
              <div class="menu-area">
                <!-- Main Menu -->
                <nav class="navbar navbar-expand-lg">
									<div class="navbar-collapse">
										<div class="nav-inner">
											<ul class="nav main-menu menu navbar-nav">
													<li class="active"><%= link_to "Home", root_url %></li>
													<li><%= link_to "Products", products_path %></li>
													<li><%= link_to "Flash Deals", flash_deals_index_path %>
													</li>
													<li><%= link_to "New Arrivals", new_arrivals_index_path %>
													</li>
													<li><a href="#">About Us<i class="fas fa-angle-down"></i></a>
														<ul class="dropdown">
															<li><%= link_to "Who We are", ourvission_index_path %></li>
                              								<li><%= link_to "Gallery", gallery_index_path %></li>
															<li><%= link_to "What's Else", ourmission_index_path %></li>
														</ul>
													</li>
													<li><%= link_to "Customer Support", contact_us_index_path %>
													</li>
													<li><a href="#">Others<i class="fas fa-angle-down"></i></a>
														<ul class="dropdown">
															<li><%= link_to "Tutorials", tutorials_index_path %></li>
                              								<li><%= link_to "Training", training_index_path %></li>
															<li><%= link_to "Project Assistance", project_assistance_index_path %></li>
														</ul>
													</li>
												</ul>
										</div>
									</div>
									
								</nav>
                <!--/ End Main Menu -->
              </div>
              
            </div>
          </div>
          
        </div>
      </div>
    </div>
    <!--/ End Header Inner -->
  </header>
  <!--/ End Header -->
<section id="contact-us" class="contact-us section">
	<div class="container">
		<div class="contact-head">
			<div class="row">
				<div class="col-lg-12 col-12">
					<div class="form-main">
						<div class="title">
							<h4>Search Result </h4>
						</div>
                        <% if @products.empty? %>
                        <h1>Oops, Product Not Found......</h1>
                        <% else %>
                        	<div class="product-info">
							<div class="tab-content" id="myTabContent">
								<!-- Start Single Tab -->
								<div class="tab-pane fade show active" id="man" role="tabpanel">
									<div class="tab-single">
										<div class="row">
											<% @products.each do |product| %> 
											<div class="col-xl-2 col-lg-4 col-md-4 col-6" style="border-radius:5px;border:1px solid rgba(0,0,0,0.3);margin-top:1rem;">
												<div class="single-product" style="margin-top:1rem;">
													<div class="product-img">
														<%= link_to product do %>
														  <%= image_tag product.product_image.url, class:"default-img", style:"width:150px;height:150px;" %>
														<% end %>
														<div class="button-head">
														<div class="product-action">
															<%= link_to product, data_toggle:"modal", data_target:"#exampleModal" do %>
															<img src="https://img.icons8.com/ios-filled/50/000000/visible.png" style="width: 35px;height: 35px;color:steelblue;margin-right: 20px;"><span>View</span>
															<% end %>
															<%= link_to product, data_toggle:"modal", data_target:"#exampleModal" do %>
															<img src="https://img.icons8.com/fluent/48/000000/shopping-bag.png" style="width: 35px;height: 35px;"><span>Buy</span>
															<% end %>
															
														</div>
														</div>
													</div>
													<div class="product-content">
														<h6 style="font-size: 15px;">
                                                        <%= link_to product do %>
                                                            <%= product.product_name.truncate(60) %>
                                                        <% end %>
                                                        </h6>
                                                        <div class="product-price">
                                                        <span><%= number_to_currency(product.sales_price, :unit => "RFW") %></span>
                                                        <% if product.regular_price.present? %>
                                                        <span class="old"><%= number_to_currency(product.regular_price, :unit => "RFW") %></span>
                                                        <% end %>
														</div>
													</div>
												</div>
											</div>
											<% end %>
										</div>
									</div>
								</div>
								<!--/ End Single Tab -->
							</div>
						</div>
                        <% end %>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
	<!--/ End Contact -->

<div class="container">
    <div class="row">
        <div class="col-md-4 shadow p-3 mb-5 bg-white rounded productshow">
          <div class="image-gallery">
            <div id="big">
              <%= image_tag @product.product_image.url %>
            </div>  
            <div id="sub">
              <% (0...@product.images.count).each do |image| %>
                <%= image_tag (@product.images[image]) %>
              <% end %>
            </div> 
          </div> 
          <script type="text/javascript">
            var bigImg = document.getElementById('big');
            var subImg = document.getElementById('sub').getElementsByTagName('img');

            for (var i = 0; i < subImg.length; i++) {
              subImg[i].addEventListener('click', full_image);
            }
            function full_image(){
              var imgSrc = this.getAttribute('src');
              bigImg.innerHTML = "<img src="+imgSrc+">";
            }
          </script>
        </div>
    <div class="col-md-6 shadow p-3 mb-5 bg-white rounded">
      <h3 id="cate" style="color:black;font-size: 15px;">
        <%= @product.product_name %>
      </h3>
      <h6 class="card-text" style="font-size: 15px;">
        <strong id="category">Product Category:</strong> <%= @product.product_category %>
      </h6>
      <h5 class="card-text" style="font-size: 15px;">
        <strong id="category">Price:</strong><%= number_to_currency(@product.sales_price, :unit => "RWF") %>
      </h5>
      <h6 id="regular" style="font-size: 15px;">
        <% if @product.regular_price.present? %>
          <%= number_to_currency(@product.regular_price, :unit => "RWF") %>
        <% end %>
      </h6>
      <h5 class="card-text" style="font-size: 15px;">
        <strong id="category">Store Name:</strong><%= @product.shop %>
      </h5>
      <h6 class="card-text" style="font-size: 15px;">
        <%= @product.stock %>
      </h6> 
      <input type="number" placeholder="1" size="3" style="border: 3px solid steelblue;font-size: 15px;"><span style="font-size: 15px;font-weight: 500;font:bold;">Quantity</span><br><br>
      <div class="col-md-12">
        <div class="col-md-4">
          <%= button_to 'Add to cart', line_items_path(product_id: @product), style:'background-color:steelblue;color:#fff;padding:15px;font-size:15px;color:#fff;
            border-radius:10px;width:150px;' %>
        </div>
        <div class="col-md-4">
          <%= button_to 'Pay Now', line_items_path(product_id: @product), class:'btn btn-primary', style:'background-color:#F7941D;color:#fff;padding:15px;font-size:15px;color:#fff;
        border-radius:10px;width:150px;' %>
      </div>
      </div>

<p id="notice"><%= notice %></p>

<h1>Sub Sub Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Product</th>
      <th>Sub category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_sub_categories.each do |sub_sub_category| %>
      <tr>
        <td><%= sub_sub_category.name %></td>
        <td><%= sub_sub_category.category.name %></td>
        <td><%= sub_sub_category.product.product_name %></td>
        <td><%= sub_sub_category.sub_category.name %></td>
        <td><%= link_to 'Show', sub_sub_category %></td>
        <td><%= link_to 'Edit', edit_sub_sub_category_path(sub_sub_category) %></td>
        <td><%= link_to 'Destroy', sub_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub Sub Category', new_sub_sub_category_path %>
